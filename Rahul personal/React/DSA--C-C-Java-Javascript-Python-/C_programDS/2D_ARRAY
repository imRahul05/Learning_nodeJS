#include<stdio.h>
int main()
{
    int i,j,m,n,a[20][30];
    printf("Enter the number of rows=");
    scanf("%d",&m);
    printf("Enter the number of columns=");
    scanf("%d",&n);
    printf("Enter the elements=");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("Elements of 2d array are-\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d",a[i][j]);
        }
        printf("\n");
    }
return 0;
}
